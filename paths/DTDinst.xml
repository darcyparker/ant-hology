<?xml version="1.0" encoding="UTF-8"?>
<project
  name="ant-hology.pathdefs.DTDinst"
  basedir="."
  xmlns:if="ant:if"
  xmlns:unless="ant:unless"
  >

  <dirname property="ant-hology.pathdefs.DTDinst.dir" file="${ant.file.ant-hology.pathdefs.DTDinst}"/>

  <import file="${ant-hology.dir}/commonProperties.xml" unless:set="ant-hology.commonProperties.loaded"/>

  <!-- See http://www.thaiopensource.com/dtdinst/ -->
  <!-- See https://code.google.com/p/jing-trang/ -->
  <path id="DTDinst.classpath">
    <pathelement location="${ant-hology.lib.dir}/dtdinst-20091111/dtdinst.jar"/>
  </path>

  <property name="ant-hology.pathdefs.DTDinst.loaded" value="true"/>

</project>
