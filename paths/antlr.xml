<?xml version="1.0" encoding="UTF-8"?>
<project
  name="ant-hology.pathdefs.antlr"
  basedir="."
  xmlns:if="ant:if"
  xmlns:unless="ant:unless"
  >

  <dirname property="ant-hology.pathdefs.antlr.dir" file="${ant.file.ant-hology.pathdefs.antlr}"/>

  <import file="${ant-hology.dir}/commonProperties.xml" unless:set="ant-hology.commonProperties.loaded"/>

  <!-- See http://www.antlr.org/ -->
  <path id="antlr.classpath">
    <pathelement location="${ant-hology.lib.dir}/antlr-4.1/antlr-4.1-complete.jar"/>
  </path>

  <property name="ant-hology.pathdefs.antlr.loaded" value="true"/>

</project>
