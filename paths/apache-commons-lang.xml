<?xml version="1.0" encoding="UTF-8"?>
<project
  name="ant-hology.pathdefs.apache-commons-lang"
  basedir="."
  xmlns:if="ant:if"
  xmlns:unless="ant:unless"
  >

  <dirname property="ant-hology.pathdefs.apache-commons-lang.dir" file="${ant.file.ant-hology.pathdefs.apache-commons-lang}"/>

  <import file="${ant-hology.dir}/commonProperties.xml" unless:set="ant-hology.commonProperties.loaded"/>

  <!-- See http://commons.apache.org/proper/commons-lang/ -->
  <path
    id="apache-commons-lang.classpath"
    path="${ant-hology.lib.dir}/apache-commons-lang-3.3.1/commons-lang3-3.1.jar"
    />

  <property name="ant-hology.pathdefs.apache-commons-lang.loaded" value="true"/>

</project>
