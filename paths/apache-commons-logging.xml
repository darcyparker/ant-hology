<?xml version="1.0" encoding="UTF-8"?>
<project
  name="ant-hology.pathdefs.apache-commons-logging"
  basedir="."
  xmlns:if="ant:if"
  xmlns:unless="ant:unless"
  >

  <dirname property="ant-hology.pathdefs.apache-commons-logging.dir" file="${ant.file.ant-hology.pathdefs.apache-commons-logging}"/>

  <import file="${ant-hology.dir}/commonProperties.xml" unless:set="ant-hology.commonProperties.loaded"/>

  <!-- See http://commons.apache.org/proper/commons-logging/ -->
  <path
    id="apache-commons-logging.classpath"
    path="${ant-hology.lib.dir}/apache-commons-logging-1.1.3/commons-logging-1.1.3.jar"
    />

  <property name="ant-hology.pathdefs.apache-commons-logging.loaded" value="true"/>

</project>
