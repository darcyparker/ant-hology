<?xml version="1.0" encoding="UTF-8"?>
<project
  name="ant-hology.pathdefs.apache-jena"
  basedir="."
  xmlns:if="ant:if"
  xmlns:unless="ant:unless"
  >

  <dirname property="ant-hology.pathdefs.apache-jena.dir" file="${ant.file.ant-hology.pathdefs.apache-jena}"/>

  <import file="${ant-hology.dir}/commonProperties.xml" unless:set="ant-hology.commonProperties.loaded"/>

  <!-- See http://jena.apache.org/ -->
  <path id="apache-jena.classpath">
    <path refid="apache-commons-httpcomponents.classpath"/>
    <path refid="apache-xerces.classpath"/>
    <pathelement location="${ant-hology.lib.dir}/apache-jena-3.0.0/lib/jena-arq-3.0.0.jar"/>
    <pathelement location="${ant-hology.lib.dir}/apache-jena-3.0.0/lib/jena-core-3.0.0.jar"/>
    <pathelement location="${ant-hology.lib.dir}/apache-jena-3.0.0/lib/jena-iri-3.0.0.jar"/>
    <!-- <pathelement location="${ant-hology.lib.dir}/apache-jena-3.0.0/lib/jena-sdb-1.5.1.jar"/> -->
    <pathelement location="${ant-hology.lib.dir}/apache-jena-3.0.0/lib/jena-tdb-3.0.0.jar"/>
    <!--See http://logging.apache.org/log4j/2.x/ -->
    <pathelement location="${ant-hology.lib.dir}/apache-jena-3.0.0/lib/log4j-1.2.17.jar"/>
    <!--See http://www.slf4j.org/ -->
    <pathelement location="${ant-hology.lib.dir}/apache-jena-3.0.0/lib/slf4j-api-1.7.12.jar"/>
    <pathelement location="${ant-hology.lib.dir}/apache-jena-3.0.0/lib/slf4j-log4j12-1.7.12.jar"/>
    <pathelement location="${ant-hology.lib.dir}/apache-jena-3.0.0/lib/jcl-over-slf4j-1.7.12.jar"/>

    <!--http://sourceforge.net/projects/jena/files/IRI/iri-0.5/-->
    <!--http://jena.sourceforge.net/iri/javadoc/com/hp/hpl/jena/iri/IRIFactory.html-->
  </path>

  <property name="ant-hology.pathdefs.apache-jena.loaded" value="true"/>

</project>


