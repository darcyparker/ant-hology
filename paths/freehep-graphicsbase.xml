<?xml version="1.0" encoding="UTF-8"?>
<project
  name="ant-hology.pathdefs.freehep-graphicsbase"
  basedir="."
  xmlns:if="ant:if"
  xmlns:unless="ant:unless"
  >

  <dirname property="ant-hology.pathdefs.freehep-graphicsbase.dir" file="${ant.file.ant-hology.pathdefs.freehep-graphicsbase}"/>

  <import file="${ant-hology.dir}/commonProperties.xml" unless:set="ant-hology.commonProperties.loaded"/>

  <!--See http://java.freehep.org/-->
  <!--Required by jeuclid-->
  <path id="freehep-graphicsbase.classpath">
    <pathelement location="${ant-hology.lib.dir}/freehep-graphicsbase-2.2.1/freehep-graphicsbase-2.2.1.jar"/>
  </path>

  <property name="ant-hology.pathdefs.freehep-graphicsbase.loaded" value="true"/>

</project>
