<?xml version="1.0" encoding="UTF-8"?>
<project
  name="ant-hology.pathdefs.hamcrest"
  basedir="."
  xmlns:if="ant:if"
  xmlns:unless="ant:unless"
  >

  <dirname property="ant-hology.pathdefs.hamcrest.dir" file="${ant.file.ant-hology.pathdefs.hamcrest}"/>

  <import file="${ant-hology.dir}/commonProperties.xml" unless:set="ant-hology.commonProperties.loaded"/>

  <!-- See https://github.com/hamcrest/JavaHamcrest -->
  <!-- required to build xmlunit -->
  <path id="hamcrest.classpath">
    <pathelement location="${ant-hology.lib.dir}/hamcrest-1.3/hamcrest-all-1.3.jar"/>
  </path>

  <property name="ant-hology.pathdefs.hamcrest.loaded" value="true"/>

</project>
