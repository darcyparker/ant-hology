<?xml version="1.0" encoding="UTF-8"?>
<project
  name="ant-hology.pathdefs.oracle-xmlparser"
  basedir="."
  xmlns:if="ant:if"
  xmlns:unless="ant:unless"
  >

  <dirname property="ant-hology.pathdefs.oracle-xmlparser.dir" file="${ant.file.ant-hology.pathdefs.oracle-xmlparser}"/>

  <import file="${ant-hology.dir}/commonProperties.xml" unless:set="ant-hology.commonProperties.loaded"/>

  <!--oracle xslt processor and xml parser are in xmlparserv2.jar-->
  <!-- See http://docs.oracle.com/cd/B14117_01/appdev.101/b12024/oracle/xml/parser/v2/oracle-xmlparser.html -->
  <path
    id="oracle-xmlparser.classpath"
    path="${ant-hology.lib.dir}/oracle-xmlparser-v2/xmlparserv2.jar"
    />

  <property name="ant-hology.pathdefs.oracle-xmlparser.loaded" value="true"/>

</project>
