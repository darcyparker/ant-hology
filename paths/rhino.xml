<?xml version="1.0" encoding="UTF-8"?>
<project
  name="ant-hology.pathdefs.rhino"
  basedir="."
  xmlns:if="ant:if"
  xmlns:unless="ant:unless"
  >

  <dirname property="ant-hology.pathdefs.rhino.dir" file="${ant.file.ant-hology.pathdefs.rhino}"/>

  <import file="${ant-hology.dir}/commonProperties.xml" unless:set="ant-hology.commonProperties.loaded"/>

  <!-- See https://developer.mozilla.org/en-US/docs/Rhino -->
  <path id="rhino.classpath">
    <pathelement location="${ant-hology.lib.dir}/rhino-1.7R4/js.jar"/>
  </path>

  <property name="ant-hology.pathdefs.rhino.loaded" value="true"/>

</project>
