<?xml version="1.0" encoding="UTF-8"?>
<project
  name="ant-hology.pathdefs.xt"
  basedir="."
  xmlns:if="ant:if"
  xmlns:unless="ant:unless"
  >

  <dirname property="ant-hology.pathdefs.xt.dir" file="${ant.file.ant-hology.pathdefs.xt}"/>

  <import file="${ant-hology.dir}/commonProperties.xml" unless:set="ant-hology.commonProperties.loaded"/>

  <!-- See http://www.blnz.com/xt/index.html -->
  <!-- See http://www.blnz.com/xt/xt-20051206/index.html -->
  <!-- See ftp://ftp.jclark.com/pub/xml/xt.zip -->
  <path
    id="xt.classpath"
    path="${ant-hology.lib.dir}/xt/xt.jar"
    />

  <property name="ant-hology.pathdefs.xt.loaded" value="true"/>

</project>
